{
  "name": "prg",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/main.js",
  "scripts": {
    "build:main": "tsc -p tsconfig.main.json",
    "dev": "concurrently \"tsc -p tsconfig.main.json -w\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron dist/main/main.js\" \"next dev src/renderer -p 5173 --turbo\"",
    "postinstall": "electron-builder install-app-deps",
    "build": "pnpm build:main && next build src/renderer",
    "start": "electron .",
    "lint": "eslint",
    "convert-charts": "node scripts/auto-convert-osu.js"
  },
  "dependencies": {
    "@react-spring/web": "^10.0.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/path-browserify": "^1.0.3",
    "@types/three": "^0.179.0",
    "adm-zip": "^0.5.16",
    "electron": "^30.0.9",
    "electron-store": "^10.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "iconv-lite": "^0.7.0",
    "jszip": "^3.10.1",
    "next": "^15.5.2",
    "osu-parsers": "^4.1.7",
    "path-browserify": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-spring": "^10.0.1",
    "simfile-parser": "^0.8.1",
    "three": "^0.180.0",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/adm-zip": "^0.5.7",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "^15.5.2",
    "mime-types": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "wait-on": "^8.0.4",
    "worker-loader": "^3.0.8"
  }
