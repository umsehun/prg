{
  "name": "prg",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/main.js",
  "scripts": {
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "next build src/renderer",
    "dev": "concurrently \"tsc -p tsconfig.main.json -w\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron dist/main/main.js\" \"next dev src/renderer -p 5173 --turbo\"",
    "postinstall": "electron-builder install-app-deps",
    "build": "pnpm build:main && pnpm build:renderer",
    "start": "electron .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit && tsc -p tsconfig.main.json --noEmit",
    "type-check:main": "tsc -p tsconfig.main.json --noEmit",
    "type-check:renderer": "tsc -p src/renderer/tsconfig.json --noEmit",
    "validate": "pnpm type-check && pnpm lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist .next src/renderer/.next src/renderer/out",
    "clean:deps": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "convert-charts": "node scripts/auto-convert-osu.js",
    "check-any-types": "grep -r \"any\" src/ --include=\"*.ts\" --include=\"*.tsx\" || echo \"No 'any' types found!\""
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "adm-zip": "^0.5.16",
    "electron": "^30.0.9",
    "execa": "^9.3.0",
    "ffmpeg-static": "^5.2.0",
    "framer-motion": "^12.5.0",
    "immer": "^10.1.1",
    "matter-js": "^0.19.0",
    "next": "^15.5.2",
    "osu-parsers": "^4.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/adm-zip": "^0.5.7",
    "@types/matter-js": "^0.19.7",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@testing-library/jest-dom": "^6.6.5",
    "@testing-library/react": "^16.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "^15.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "prisma": "^6.1.0",
    "tailwindcss": "3.4.13",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "wait-on": "^8.0.4"
  }
}